	   {
	  if(ejecutar == true) {
	    if(xVar.image != null && mapaVariables.containsKey(xVar.image)) {
		  xInt = mapaVariables.get(xVar.image);
		} else if (xVar.image != null && !mapaVariables.containsKey(xVar.image)) {
